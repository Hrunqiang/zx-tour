@charset "utf-8";
html,body, h1, h2, h3, h4, h5, h6, hr, p, blockquote,dl, dt, dd, ul, ol, li,pre, fieldset, lengend, button, input, textarea, th, td ,header,footer{ 
    margin: 0;
    padding: 0;
}

body,button, input, select, textarea{ 
    font: 12px/1 "黑体",Arial,Tahoma, Helvetica, sans-serif; 
}

h1 { font-size: 30px; text-align: center;}
h2 { font-size: 14px;font-style: normal;font-weight: normal; }
h3 { font-size: 14px; }
h4, h5, h6 { font-size: 100%; }

address, cite, dfn, em, var { font-style: normal; } /* 将斜体扶正 */
code, kbd, pre, samp, tt { font-family: "Courier New", Courier, monospace; } /* 统一等宽字体 */
small { font-size: 12px; } /* 小于 12px 的中文很难阅读，让 small 正常化 */

/* 重置列表元素 */
ul, ol { list-style: none; }


a,a:link,a:visited,a:hover,a:active{text-decoration: none; color:#FFF;cursor: pointer;}


/* 重置表单元素 */
legend { color: #000; }
img { border: none;width: 100%;display: inline-block;}

button, input, select, textarea {
    font-size: 12px; 
}

/* 重置表格元素 */
table {
    border-collapse: collapse;
    border-spacing: 0;
}

/* 重置 hr */
hr {
    border: none;
    height: 1px;
}
:focus{ outline:0 }
/*input[placeholder], [placeholder], *[placeholder] { 
    font-size: 12px;
}*/
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    font-size: 12px; 
}

::-moz-placeholder { /* Mozilla Firefox 19+ */
    font-size: 12px;
}

input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    font-size: 12px;
}

input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    font-size: 12px;
}
button{cursor: pointer;}
.w220{width: 220px;}
html { overflow-y: scroll; }
.section{position: relative;overflow: hidden;}
.checkbox{vertical-align:middle;margin-top:-3px;height: 12px;cursor: pointer;}
.section_center{width: 1100px;margin:0 auto;}
.fl{float: left;}
.vertical_center{position: relative;overflow: hidden;}
.strip{background: #01BFF1;height: 40px;line-height: 40px;color:#FFF;}
#header_strip{text-align: right;}
#header_focus_box{
    background-image:url(/static/images/index_1.jpg);
    background-size: cover;
    background-repeat: no-repeat;
    position: relative;
    -webkit-transition: background 2s linear;
       -moz-transition: background 2s linear;
        -ms-transition: background 2s linear;
         -o-transition: background 2s linear;
            transition: background 2s linear;
}
#header_box{height: 400px;background-image:url(/static/images/index_2.jpg) ;position: relative;background-size: cover;background-repeat: no-repeat;border-bottom: 10px solid #01BFF1;}
#header_center{height: 100%;}
#header_center_npa{width: 300px;height: 40px;background:#01BFF1;position: absolute;bottom: 0; line-height: 40px;font-size: 14px;color: #FFF;}
#header_center_npa span{font-size: 20px;padding-left:30px;}
#header_focus_box #header_focus_word{color:#FFF;padding:100px 0;}
#header_focus_box #header_focus_word p{font-size: 40px;height: 40px;margin-bottom: 30px;}
#header_focus_box #header_focus_word a{background: #FF9E01;font-size: 24px;height: 80px;padding:10px 20px;}
#header_focus_box .focus-item{width: 100%;position: absolute;bottom:100px;}
#header_focus_box .focus-item ul{height: 20px;margin:0 auto;}
#header_focus_box .focus-item ul li{float: left;background: #FFF;height: 16px;width: 16px;margin:0 10px;position: relative;
    -webkit-border-radius: 50%;
       -moz-border-radius: 50%;
        -ms-border-radius: 50%;
         -o-border-radius: 50%;
            border-radius: 50%;
}
#header_focus_box .focus-item ul .curr,#header_focus_box .focus-item ul li:hover{
    background: #FF9E00;
    -webkit-transition: background 1s linear;
       -moz-transition: background 1s linear;
        -ms-transition: background 1s linear;
         -o-transition: background 1s linear;
            transition: background 1s linear;
}
#index_nav_box{height: 80px;position: absolute;left:0;width: 100%;background: #72D0F4;}
#header_focus_box #index_nav_box{bottom:0;}
#header_box #index_nav_box{top:40px;}
.nav{height: 100%;color:#FFF;}
.nav #nav_logo{height: 100%;width: 200px;font-size: 30px;line-height: 80px;font-weight: bold;}
.nav .nav-item{height: 60px;margin-top:10px;float: right;}
.nav .nav-item>li{float: left;font-size: 20px;width: 140px;height: 100%;line-height: 60px;text-align: center;}
.nav .nav-item a{display:inline-block;height: 40px;line-height: 40px;padding:0 30px;}
.nav .nav-item>li>a{margin-top:10px;}
.nav ul,.nav .nav-item li,#header_focus_box #header_focus_word a,.nav .nav-item a{
    -webkit-border-radius: 5px;
       -moz-border-radius: 5px;
        -ms-border-radius: 5px;
         -o-border-radius: 5px;
            border-radius: 5px;
}
.nav .nav-item>li:hover a{background: #FFF; color:#72D0F4;}
.nav .nav-item>li{position: relative;}
.nav .nav-item li ul{position: absolute;background: #FFF;left:0;display: none;min-width: 150px;
padding:10px 0;}
#header_focus_box .nav .nav-item li ul{bottom:60px;}
#header_box .nav .nav-item li ul{top:60px;}
.nav .nav-item li ul li{height: 40px;line-height: 40px;}
.nav .nav-item li ul:before{
    content:'';
    position: absolute;
    left:30px;
}
#header_focus_box .nav .nav-item li ul:before{
    bottom:-9px;
    border-right: 9px solid transparent;  
    border-left: 9px solid transparent; 
    border-top: 9px solid #FFF;
}
#header_box .nav .nav-item li ul:before{
    top:-9px;
    border-right: 9px solid transparent;  
    border-left: 9px solid transparent; 
    border-bottom: 9px solid #FFF;
}
/*.nav .nav-item li:hover ul {display:block;transition: background 1s linear;}*/
.nav .nav-item li ul li:hover a{background: #72D0F4;color:#FFF;}
