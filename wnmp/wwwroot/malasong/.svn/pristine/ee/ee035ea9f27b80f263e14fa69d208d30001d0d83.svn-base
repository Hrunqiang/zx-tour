.step2 .jdyd_list  .item_content .jd_fangjian li.cls5{padding-top:10px;width:50px;}
.step2 .jdyd_list  .item_content .jd_fangjian li.cls5 input{width:40px;height:32px;text-align: center;line-height:32px}

.order_price{padding-bottom: 30px;}
