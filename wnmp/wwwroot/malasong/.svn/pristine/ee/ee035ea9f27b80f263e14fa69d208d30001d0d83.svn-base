.step2 .jdyd_list .item .txt  .fuli .icon1{background: url(/static/images/jd_wifi.png) no-repeat; width:100px; height:25px; padding-top:12px; text-align:right;overflow: hidden;}
.step2 .jdyd_list .item .txt  .fuli .icon2{margin-left:24px;display:inline;background: url(/static/images/jd_zaodian.png) no-repeat; width:100px; height:25px; padding-top:12px; text-align:right;overflow: hidden;}
.step2 .jdyd_list .item .txt  .fuli .icon3{margin-left:24px;display:inline;background: url(/static/images/jd_tingche.png) no-repeat; width:85px; height:25px; padding-top:12px; text-align:right;overflow: hidden;}
.step2 .jdyd_list .item  .btn1{width:100px;height:35px;padding-top:15px;font-size:16px;}
.step2 .jdyd_list .item  .btn2{width:100px;height:35px;padding-top:15px;font-size:16px;margin-top:68px;cursor: pointer;}
.step2 .jdyd_list .item .jd_des{ padding-top:20px;}
.step2 .jdyd_list  .item_content{width:696px;margin:0 auto;border-top: 1px dashed #c2c2c2;margin-top:30px;padding-bottom:30px;display:none;}
