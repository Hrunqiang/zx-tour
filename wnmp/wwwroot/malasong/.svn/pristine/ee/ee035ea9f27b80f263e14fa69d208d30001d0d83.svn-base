.enroll{background-color: #fff;width:67.5%; color:#505960;/* font-family: "宋体" */}
input, select , option,textarea,button {text-transform: none;font: 12px/1.5 "Microsoft YaHei",tahoma,arial,Hiragino Sans GB,\5b8b\4f53,sans-serif;}
.enroll{background-color: #fff;width:65%; color:#505960;/* font-family: "宋体" */}
.enroll .title{text-align: center;font-size:22px;font-weight: bold;padding-top:28px;}
.enroll  .title_des{color:#fead42;text-align: center;padding-top:30px;font-size:12px;padding-bottom:60px;}
.enroll ul li{padding-left:94px;font-size:14px;font-weight: bold;}
.enroll ul li .normal_input{width:260px;height:22px;color:#a0a0a0;border:1px solid #c2c2c2;padding:10px;}
.enroll ul li .normal_select{width:280px;height:44px;color:#a0a0a0;border:1px solid #c2c2c2;padding-left:10px;padding-right:10px;}
.enroll ul li .left_input{width:120px;height:22px;color:#a0a0a0;border:1px solid #c2c2c2;padding:10px;}
.enroll ul li .left_select {width:130px;height:44px;color:#a0a0a0;border:1px solid #c2c2c2;padding-left:10px;padding-right:10px;}
.enroll ul li .space{margin-left:22px;display: inline;}
.enroll ul li span{font-size:12px;color:#a0a0a0;font-weight: normal;padding-left:16px;}
.enroll ul li .des{font-size:14px;color:#a0a0a0;font-weight: normal;padding:8px;padding-top:16px;text-align: center;}
.enroll ul li label{color:#a0a0a0;font-weight: normal;}
.enroll ul li .label{width:140px;padding-bottom:16px;}
.enroll ul li.txt{padding-bottom:14px;}
.enroll ul li.line{padding-bottom:30px;}
.enroll ul li .bmtype{width:32%;text-align:center;line-height: 20px;}
.enroll ul li .bmtype .radio_price span{font-size:18px;padding:0;}
.enroll ul li .bmtype .radio_price{padding-top:8px;}
.enroll  div.line{width:696px;border-bottom: 1px solid #a0a0a0;height:1px;margin:0 auto;}
.enroll .submit{width:100%;text-align:center;margin:0 auto;padding-bottom:30px;}
.enroll .submit input{width:160px;height:44px;border:0;background-color: #ff9e01;color:#fff;font-size:18px;cursor: pointer;}

.enroll .radius5{
		 -webkit-border-radius: 5px;
       -moz-border-radius: 5px;
        -ms-border-radius: 5px;
         -o-border-radius: 5px;
            border-radius: 5px;
}
.step2 ul li .pepo{color:#01bff1;font-weight: normal;padding-top:8px;margin-right:20px;display: inline;} 
.step2 ul li .addPepo{font-weight: normal;background-color: #ff9e01;display: inline;padding:8px;color:#fff;cursor:pointer;
	 -webkit-border-radius: 5px;
       -moz-border-radius: 5px;
        -ms-border-radius: 5px;
         -o-border-radius: 5px;
            border-radius: 5px;
}

.step2 ul li.content{font-weight: normal;line-height: 24px; }
.step2 ul li.content .bg{background-color: #fafafa;width:82%;padding:10px;}
.step2 ul li.content .contxt{color:#a0a0a0;}
.step2 ul li.jdyd {padding-top:30px;padding-bottom:30px;}
.step2 ul li.jdyd  .t{font-weight: normal;font-size:16px;padding-left:5px;}
.step2 ul li.jdyd  .tp{font-weight: normal;font-size:12px;padding-top:5px;}

.step2 .jdyd_list{display: none;}
