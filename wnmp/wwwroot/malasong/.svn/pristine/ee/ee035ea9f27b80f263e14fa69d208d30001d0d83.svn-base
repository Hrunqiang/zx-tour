.step2 .jdyd_list  .item_content .jd_fangjian li.cls3{width:304px;padding-top:10px;}
.step2 .jdyd_list  .item_content .jd_fangjian li.cls3 .selectDate{width:100px;height:32px;line-height:32px;padding-left:5px;padding-right:5px;text-align: center;}
