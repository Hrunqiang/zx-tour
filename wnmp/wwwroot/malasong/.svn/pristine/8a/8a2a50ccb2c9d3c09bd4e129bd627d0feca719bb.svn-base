#header_focus_box #header_focus_word{color:#FFF;padding:100px 0;}
#header_focus_box #header_focus_word p{font-size: 40px;height: 40px;margin-bottom: 30px;}
#header_focus_box #header_focus_word a{background: #FF9E01;font-size: 20px;height: 80px;padding:10px 20px;}
#header_focus_box .focus-item{width: 100%;position: absolute;bottom:70px;}
#header_focus_box .focus-item ul{height: 20px;margin:0 auto;}
#header_focus_box .focus-item ul li{float: left;background: #FFF;height: 12px;width: 12px;margin:0 10px;position: relative;
    -webkit-border-radius: 50%;
       -moz-border-radius: 50%;
        -ms-border-radius: 50%;
         -o-border-radius: 50%;
            border-radius: 50%;
}
#header_focus_box .focus-item ul .curr,#header_focus_box .focus-item ul li:hover{
    background: #FF9E00;
    -webkit-transition: background 1s linear;
       -moz-transition: background 1s linear;
        -ms-transition: background 1s linear;
         -o-transition: background 1s linear;
            transition: background 1s linear;
}
#index_nav_box{height: 60px;position: fixed;left:0;width: 100%;background:rgb(45,178,234);background: rgba(45,178,234,.8);top:40px;z-index: 998;}
#header_focus_box #index_nav_box{}
#header_box #index_nav_box{top:40px;}
.nav{height: 100%;color:#FFF;position: relative;}
.nav #nav_logo{height: 100px;width: 100px;position: absolute;}
#header_focus_box #nav_logo{bottom:10px;}
#header_box #nav_logo{top:-30px;display: block;}
.nav #nav_logo img{width: 100%;position: absolute;}
#header_focus_box .nav #nav_logo img{bottom:0;}
#header_box .nav #nav_logo img{top:0;}
.nav .nav-item{height: 50px;margin-top:5px;float: right;}
.nav .nav-item>li{float: left;font-size: 16px;width: 140px;height: 100%;line-height: 50px;text-align: center;
height: 50px}
.nav .nav-item a{display:inline-block;height: 30px;line-height: 30px;padding:0 20px;}
.nav .nav-item>li>a{margin-top:5px;padding:0 30px;}
.nav ul,.nav .nav-item li,#header_focus_box #header_focus_word a,.nav .nav-item a{
    -webkit-border-radius: 5px;
       -moz-border-radius: 5px;
        -ms-border-radius: 5px;
         -o-border-radius: 5px;
            border-radius: 5px;
}
.nav .nav-item>li:hover a{background: #FFF; color:#72D0F4;}
.nav .nav-item>li{position: relative;}
.nav .nav-item li ul{position: absolute;background: #FFF;left:7px;display: none;
padding:10px;}
#header_focus_box .nav .nav-item li ul{top:47px;}
#header_box .nav .nav-item li ul{top:47px;}
.nav .nav-item li ul li{height: 30px;line-height: 30px;text-align: left;}
.nav .nav-item li ul:before{
    content:'';
    position: absolute;
    left:30px;
}
/*#header_focus_box .nav .nav-item li ul:before{
    bottom:-5px;
    border-right: 5px solid transparent;  
    border-left: 5px solid transparent; 
    border-top: 5px solid #FFF;
}*/
#header_box .nav .nav-item li ul:before,#header_focus_box .nav .nav-item li ul:before{
    top:-5px;
    border-right: 5px solid transparent;  
    border-left: 5px solid transparent; 
    border-bottom: 5px solid #FFF;
}
/*.nav .nav-item li:hover ul {display:block;transition: background 1s linear;}*/
.nav .nav-item li ul li:hover a{background: #72D0F4;color:#FFF;}
