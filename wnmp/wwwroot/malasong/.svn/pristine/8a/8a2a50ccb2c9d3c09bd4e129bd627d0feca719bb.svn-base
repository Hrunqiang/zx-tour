    margin:-225px 0 0 -200px;
    box-shadow: 0px 0px 40px rgba(0,0,0,.8);
}
.coverlayer_closed{
    position: absolute;
    width: 20px;
    width: 30px \9;
    height: 20px;
    cursor: pointer;
    top:20px;
    right:30px;
}
.coverlayer_closed:before,.coverlayer_closed:after{
    content: "";
    width: 2px;height: 20px;
    background: #757575;
    background: #FFF \9;
    position: absolute;
    left:9px;
}
.coverlayer_closed:before{
    -webkit-transform: rotate(45deg);
       -moz-transform: rotate(45deg);
        -ms-transform: rotate(45deg);
         -o-transform: rotate(45deg);
            transform: rotate(45deg);
}
.coverlayer_closed:after{
    -webkit-transform: rotate(-45deg);
       -moz-transform: rotate(-45deg);
        -ms-transform: rotate(-45deg);
         -o-transform: rotate(-45deg);
            transform: rotate(-45deg);
}
