.flexBox{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-align: center;-webkit-align-items: center;-ms-flex-align: center;align-items: center;}
.flex_1{flex:1;-webkit-box-flex: 1;-webkit-flex: 1;-ms-flex: 1;}
.flex_2{flex:2;-webkit-box-flex: 2;-webkit-flex: 2;-ms-flex: 2;}
.flex_3{flex:3;-webkit-box-flex: 3;-webkit-flex: 3;-ms-flex: 3;}

.matchBox{overflow: hidden;background: #FFF;}
.matchBox .weui_media_hd{width: 8.92857rem!important;height: 6.714285rem!important;}
