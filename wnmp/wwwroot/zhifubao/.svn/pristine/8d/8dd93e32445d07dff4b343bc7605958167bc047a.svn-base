@charset "utf-8";

/*科e互联特效基本框架CSS结束，应用特效时，以上样式可删除*/
.page {display: none;position: fixed;left: 0;bottom: 0;right: 0;width: 100%;height: 100%;overflow: hidden;}
/*#datescroll div{float: left;margin-left: 10%;margin-top: 15px;padding-right: 22px;}*/
#datescroll div{float: left;/*margin-top: 15px;*/padding-right: 22px;}

#yearwrapper{position: absolute;left: 5%;top:2.857142857142857rem;bottom: 0px;width:30%;}
#monthwrapper{position: absolute;left: 35%;top:2.857142857142857rem;bottom: 0px;width:30%;}
#daywrapper{position: absolute;left: 65%;top:2.857142857142857rem;bottom: 0px;width:30%;}
/*#monthwrapper{position: absolute;left: 26%;top:45px;bottom: 60px;width:100%;}
#daywrapper{position: absolute;left: 50%;top:45px;bottom: 60px;width:100%;}
*/
#Hourwrapper ul li{color: #898989;font-size: 12px;}
#Minutewrapper ul li{color: #898989;font-size: 12px;}
#Secondwrapper ul li{color: #898989;font-size: 12px;}
#yearwrapper ul li{color: #000;font-size: 1.428571428571429rem;height: 2.857142857142857rem;overflow: hidden;}
#monthwrapper ul li{color: #000;font-size: 1.428571428571429rem;height: 2.857142857142857rem;overflow: hidden;}
#daywrapper ul li{color: #000;font-size: 1.428571428571429rem;height: 2.857142857142857rem;overflow: hidden;}

#markyear{position:relative; margin-left: 76px;top:-2px;border-top: 1px solid red;}
#markmonth{position:relative; margin-left: 40px;top:-2px;}
#markday{position:relative; margin-left: 42px;top:-2px;}
#markhour{position:relative; margin-left: 62px;top:-2px;}
#markminut{position:relative; margin-left: 58px;top:-2px;}
#marksecond{position:relative; margin-left: 68px;top:-2px;}
#dateheader{width: 100%;height: 50px;background: #04BE02;text-align: center;color: #fff;line-height: 50px;font-size: 20px;}

#dateshadow{display: none;position: absolute;width: 100%;height: 70.2642857142rem;top:0;left:0;background: #888; filter:alpha(Opacity=50);-moz-opacity:0.5;opacity: 0.5;z-index: 999;}
#datePage{font-size: 12px; /*border-radius: 3px; position:absolute;*/vertical-align:middle;bottom:0;width: 100%;height: 20rem;background: #FFFFFF;z-index:9999999;}
#datetitle{width: 100%;height:40px;background: #e3e5e9;text-align: center;color: #fff;line-height: 40px;font-size: 20px;font-family:'microsoft yahei';}
#datetitle h1{font-weight:normal;}
#datemark{font-size: 18px;left:0%;width: 100%;height: 2.428571428571429rem;position:absolute;top:8.642857142857143rem;/*background:#eee;*/border:1px solid #eee;border-bottom: 1px solid #acaeb1;border-top: 1px solid #acaeb1;}
#timemark{font-size: 18px;left:5%;width: 90%;height: 1.428571428571429rem;position:absolute;top:17.28571428571429rem;background:#eee;border:1px solid #eee;}
#datescroll{width:100%;height: 21.42857142857143rem;text-align: center;line-height: 40px;background: #d1d4db;color: #898c93;}
#yearwrapper ul,#monthwrapper ul,#daywrapper ul{width:100%;}

#datetitle h1{font-size:18px;}


#dateFooter{width:100%;height: 3.142857142857143rem;/*bottom: 0px;*/position: absolute;border-bottom: 1px solid #acaeb1;border-top: 1px solid #acaeb1;z-index: 1000;}
#setcancle ul{text-align: center;line-height: 40px;font-size: 20px;}
#setcancle ul li{/*border-radius:3px;*/float: left;width: 47.6%;height: 3.142857142857143rem;list-style-type: none;font-family:'microsoft yahei';font-size:1.142857142857143rem;background: #e3e5e9;}
#dateconfirm{background:#04BE02;color:#00AAEE;text-align: right;line-height: 3.142857142857143rem;padding-right: 2.4%;}
#datecancle{background:#dcdddd;color:#a1a1a1;text-align: left;line-height: 3.142857142857143rem;padding-left: 2.4%;}