<script type="text/javascript">
weui.search_bar.init();
$(".banner_pic").silder();
$(window).resize(function(){
    // windW = $(".wrap").width();
    // $(".banner").silder();
});
// $(".change_hot").tap(function(){
//     alert('e');
//     return false;
// });
///////////////////////////热词切换//////////////////
$(document).ready(function(){
	$('#search_input').focus(function () {
		var Scro=$('.search_default').height();
		var Scro_top=$('#hotword_view').height();
		var num=0;
		$('.change_hot').click(function  () {
			var hot=document.getElementById('hotword');
			num++;
			if(Scro*num>Scro_top){
				num=0;
			}
			hot.scrollTop=Scro*num;
		})
	})	
})
sharedes = "全球跑步赛事，一站直达";
timelinetitle = "知行合逸 - 全球跑步赛事，一站直达";
//wxbind();
////////////////////////////////////////滚动加载/////////////////
var now = "<{$now}>";
var Page=1;
	$(document).ready(function  () {
		///////获取窗口高度
		var winH=$(window).height();
		///////防止多次触发ajax请求
		var stop=true;
		/////////////滚动获取页面高度
		$(window).scroll(function(){
			var pageH=$('.wrap').height();
			var scrollT=window.scrollY;
			var aa=(pageH-winH-scrollT)/winH;
//			console.log(aa);判断是否到达底部
			if(aa<0.2){
//				$('.loading').remove();
				console.log(1111);
				if(stop==true){
					stop=false;
					Page++;
					var htm_str='<div class="loading"><div class="jz">加载中</div><div class="weui_loading"><div class="weui_loading_leaf weui_loading_leaf_0"></div><div class="weui_loading_leaf weui_loading_leaf_1"></div><div class="weui_loading_leaf weui_loading_leaf_2"></div><div class="weui_loading_leaf weui_loading_leaf_3"></div><div class="weui_loading_leaf weui_loading_leaf_4"></div><div class="weui_loading_leaf weui_loading_leaf_5"></div><div class="weui_loading_leaf weui_loading_leaf_6"></div><div class="weui_loading_leaf weui_loading_leaf_7"></div><div class="weui_loading_leaf weui_loading_leaf_8"></div></div></div>';
					$('.hot_matchs').append(htm_str);
						$.ajax({
						type:"get",
						url:"/ajax/hotlist",
						data:{page:Page},
						async:true,
						dataType:"json",
						success:function(obj){
//							console.log(obj);
							if(obj.error==0){
	//							console.log(obj.data);
								var Obj=obj.data;
								var str='';
								setTimeout(function () {
									$.each(Obj, function(key,val) {
									var minge='';
									if(Obj[key].m_state!=0 || Obj[key].m_GameTime <=now || Obj[key].m_releaseTime >now || Obj[key].m_untilTime<now || Obj[key].m_offineTime <now){
										var minge='<span class="match_tips match_tips_pay1">名额已满</span>';
									}else if(Obj[key].m_placesleft<=20){
										var minge='<span class="match_tips match_tips_warm match_tips_pay2">名额紧张</span>';
									}
								var Time=Obj[key].m_GameTime.substring(0,10);
								var types=Obj[key].m_Mtypes.replace(/\|/g,'<span></span>');
								var str='<a href="/Matchinfo?m_id='+Obj[key].id+'" class="weui_media_box weui_media_appmsg"><div class="weui_media_hd"><img class="weui_media_appmsg_thumb" src='+Obj[key].m_img+' alt=""></div><div class="weui_media_bd"><h4 class="weui_media_title flex"><span class="S_overhiden">'+Obj[key].m_name+'</span>'+minge+'</h4><p class="weui_media_desc weui_media_desc_1 line2 weui_media_title_position">'+Obj[key].m_des+'</p><p class="weui_media_desc weui_media_desc_padding">'+Time+'  '+types+'</p></div></a>';
								$('.hot_matchs').append(str);	
								$('.loading').remove();
								stop=true;
//								console.log(111);
								});
							},1000);
							}else if(obj.error==1){
								$('.loading').remove();
								var jz_succeed='<div class="loading">已加载完毕<div>';
								$('.hot_matchs').append(jz_succeed);
							}
						}
					});
				}
			}
		})
		
	})
</script>
</body>
</html>