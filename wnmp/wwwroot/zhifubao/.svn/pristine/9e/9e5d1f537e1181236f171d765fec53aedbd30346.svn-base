<script type="text/javascript">
$(document).ready(function(){
	window.totalDist = '<{$info.totalDist}>';
	window.headimgurl = '<{$usr.headimgurl}>';
	window.headimgurl = window.headimgurl ?window.headimgurl :"http://weixin.zx-tour.com/h5/run/images/weixin_avatar.jpg";
	if(!window.totalDist){
		sharetitle = "快来参加约跑42.195！波马限量跑鞋在召唤你！";
	}
    $('.float_btns').click(function(){
		$('.share').show(100);
	});
	$('.share').click(function(){
		$(this).hide(100);
	});

	//默认提示框
	var activity_over = {
		'exchangeState':<{$price_info.exchangeState}>,        
        'is_expire' : '<{$price_info.is_expire}>',  //是否中奖
		'gift_name' : '<{$price_info.gift_name}>',
		'rank_hunt':'<{$price_info.rank_hunt}>',      
		'ems': "<{$price_info.ems}>",
	};
	function load_prompt(){
		var isinput = false;
		var str = '<div class="default_prompt"><div class="cont"><div class="notice">';
		str += '活动结束啦！<br />感谢您参与知行合逸约跑42195活动！<br />'+activity_over.rank_hunt+'</div><dl><dt style="text-align:center">'+ activity_over.gift_name +'</dt>';
		if(activity_over.ems == 'false'){
			str += '<dd style="line-height:16px;">请在淘宝店铺中搜索：跑者装备企业店<br>进店使用</dd><a class="btn">确定</a></div></div><div class="bg"></div></div>';
		}else{
			if(activity_over.exchangeState){
				str += '<a class="btn">已兑奖</a></div></div><div class="bg"></div></div>'; 
			}else{
				str +='<dd style="line-height:20px;">请填写以下资料：</dd>';
				str += '<dd><input type="text" value="11" placeholder="姓名" name="user_name"/></dd>';
				str += '<dd><input type="text" value="12322221111" placeholder="手机号" name="user_phone"/></dd>';
				str += '<dd><input type="text" value="addraddr"  placeholder="地址" name="user_address"/></dd>';
				str += '<dd style="line-height:16px;">请务必在5月3日前完成邮寄信息的填写</dd>';
				str += '</dl><div class="btns">';
	            str +=  '<a class="btn" id="done_btn">确 定</a></div></div><div class="bg"></div></div>';
				isinput = true;
			}
		}
		$('body').append(str);
		if(isinput == false){
			$('.default_prompt .btn').click(function(){
				$(this).parents('.default_prompt').remove();
			});
		}
	}
	if(activity_over.is_expire == 'true'){
		load_prompt();
	}
	var data = {};
	$('#done_btn').click(function(){
		var name = $(this).parent().parent().find('input[name="user_name"]').val();
		var phone = $(this).parent().parent().find('input[name="user_phone"]').val();
		var address = $(this).parent().parent().find('input[name="user_address"]').val();
		if(name != null && name.length == 0){alert("请输入姓名");return;}
		if(phone != null && phone.length == 0){alert("请输入手机号");return;}
		if(phone != null && phone.length != 11){alert("请输入11位手机号");return;}
		if(address != null && address.length == 0){alert("请输入邮寄地址");return;}

		if(phone != null && phone.length != 0){data.user_phone=phone;}
		if(name != null && name.length != 0){data.user_name=name;}
		if(address != null && address.length != 0){data.user_address=address;}

        $('#exchange_box').attr("style", "display:block;z-index:100000;");					
	});

    $('#cancelBtn').click(function(){
        $('#exchange_box').attr("style", "display:none;");
    });
    $('#sureBtn').click(function(){     
        $('#exchange_box').attr("style", "display:none;");
        $.ajax({
			type: "POST",
			 url : "/WxRuner/addems",
			 data:data,
			dataType: "json",
			  success: function(data){
			  	if(data.error == "1"){
			  		$('.default_prompt').remove();
                    location.reload();
			  	}else{
			  		alert(data.msg);
			  	}
			  }
		}); 
    }); 
	
});

var startpage=parseInt('2'),
count=<{$count}>,
ajaxload=false,
winHeight=$(window).height(),
maxpage=false;
$(window).scroll(function(){
	var wst=$(window).scrollTop();
	var bodyHeight=$(document).height();
	if(!ajaxload){
		if(wst+winHeight>bodyHeight-10){
			ajaxload=true;
			ajaxtip("努力加载中...");
			$.ajax({
			  url: '/WxRuner/ranklist/',
			  dataType: 'json',
			  type:'POST',
			  data:{currentPage:startpage},
			  success: function(data){
		  		var bookshtml="";
		  		startpage = data.startpage;
    			var list = data.list;
    			for(var key in list){
    				count++;
    				bookshtml+='<li>\
									<span>'+count+'</span>\
									<span>'+list[key].username+'</span>\
									<span>'+list[key].bestScore+'</span>\
								</li>';
			  	}
				$("#ranklist").append(bookshtml);
				ajaxtip("加载完毕");
				ajaxload=false;
			  },
			  error: function(xhr, type){
			  	ajaxload=false;
			    ajaxtip("加载失败，请稍候重试！");
			  }
			})
		}
	}
});
function ajaxtip(txt){
	$(".get_more").html(txt);
}
